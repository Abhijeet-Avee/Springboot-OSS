package com.bookapp.model.entities;

import lombok.AllArgsConstructor;
import lombok.Data;

@Data
public class Book {
	private Integer id;
	private String title;
	private Double price;
	private String author;
	public Book(String title, Double price, String author) {
		super();
		this.title = title;
		this.price = price;
		this.author = author;
	}
	
	
}
